  <builders>
    <hudson.plugins.python.Python plugin="python@1.2">
      <command>import os
import subprocess
import glob

print &quot;++++++++++++++++++ Get last user +++++++++++++++++++&quot;

changelist = os.environ[&apos;P4_CHANGELIST&apos;]
print changelist

proc = subprocess.Popen([&quot;p4&quot;,  &quot;changes&quot;, &quot;-m 2&quot; , &quot;//Wireshark_Spirent/mainline/src&quot; +  &quot;/...&quot;], stdout=subprocess.PIPE)
output = proc.stdout.read()
junk, junk, junk, junk, junk, user, junk = output.split(&apos; &apos;, 6)
print &quot;user string is: &quot;, user
user1, junk = user.split(&apos;@&apos;, 2)
print &quot;last change is submitted by &quot;, user1

print &quot;++++++++++++++++++ Check if last user is tableuser. If yes then bash command to build 32-bit wireshark won&apos;t be executed. +++++++++++++++++++++++&quot;

# delete previous files
for filename in glob.glob(&quot;runbash.txt&quot;):
        os.remove(filename)

if user1 == &apos;tableuser&apos;:
    fo = open(&quot;runbash.txt&quot;, &apos;w&apos;)
    fo.close();</command>
    </hudson.plugins.python.Python>
    <org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder plugin="conditional-buildstep@1.3.3">
      <runner class="org.jenkins_ci.plugins.run_condition.BuildStepRunner$Fail" plugin="run-condition@1.0"/>
      <runCondition class="org.jenkins_ci.plugins.run_condition.core.FileExistsCondition" plugin="run-condition@1.0">
        <file>$WORKSPACE/runbash.txt</file>
        <baseDir class="org.jenkins_ci.plugins.run_condition.common.BaseDirectory$Workspace"/>
      </runCondition>
      <conditionalbuilders>
        <hudson.tasks.Shell>
          <command>echo &quot;Last user is not tableuser. Continue.&quot;
sudo chmod -R 777 .
sudo ./autogen.sh
sudo ./configure 
sudo make</command>
        </hudson.tasks.Shell>
        <hudson.tasks.Shell>
          <command>#echo &quot;Packaging&quot;
#p4 edit //Wireshark_Spirent/mainline/src/config.nmake
#p4 change -o | sed &apos;s/-spirent-3/-spirent-4/g&apos; config.nmake &gt; config.nmake
#p4 submit -d &quot;$BUILD_NUMBER $P4_CHANGELIST&quot;
</command>
        </hudson.tasks.Shell>
      </conditionalbuilders>
    </org.jenkinsci.plugins.conditionalbuildstep.ConditionalBuilder>
