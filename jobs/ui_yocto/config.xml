    <hudson.tasks.BatchFile>
      <command>del UI_changelist_*.txt
echo %P4_CHANGELIST% %JOB_NAME% %BUILD_NUMBER% &gt; UI_changelist_%P4_CHANGELIST%.txt</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>echo STC BUILD ROOT is %STC_BUILD_ROOT%
mkdir __ui_only_build__
del /Q TestCenter.Windows.bll.tar.gz
del BLL_changelist*.txt
exit /B 0</command>
    </hudson.tasks.BatchFile>
    <hudson.plugins.copyartifact.CopyArtifact plugin="copyartifact@1.30">
      <project>bll_windows_yocto</project>
      <filter>bin/Release/TestCenter.Windows.bll.tar.gz, BLL_changelist*.txt</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastSuccessfulBuild</id>
      </selector>
      <flatten>true</flatten>
      <doNotFingerprintArtifacts>false</doNotFingerprintArtifacts>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact plugin="copyartifact@1.30">
      <project>bll_windows_yocto</project>
      <filter>bin/Release/STAKCommands.beta.Windows.tar.gz</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <flatten>true</flatten>
      <doNotFingerprintArtifacts>true</doNotFingerprintArtifacts>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.BatchFile>
      <command>python framework\tools\buildUtil\package.py --install TestCenter.Windows.bll.tar.gz --directory=bin/Release
attrib -r C:\jenkins\workspace\ui\bin\Release\phxrpc.dll

python framework\tools\buildUtil\package.py --install STAKCommands.beta.Windows.tar.gz --directory=bin/Release --append</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>python genSln.py</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>pushd C:\Program Files (x86)\Microsoft Visual Studio 10.0\VC
call vcvarsall.bat x86
popd
msbuild.exe TestCenter.UI.Gen.sln /t:build /p:Configuration=Release
</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>cd bin\Release
rebase.exe -b 0x12000000 -u . *.dll
exit /B 0</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>cd bin\Release
del TestCenter*.tar.gz
if %BUILD_PACKAGE_BUILD% == true (
xcopy /Y /R /D ..\..\scm\tools\devexpress .
xcopy /Y /R /D ..\..\scm\tools\dundas .
xcopy /Y /R /D ..\..\scm\tools\janus .
python ..\..\framework\tools\buildUtil\package.py --package=TestCenter 
python ..\..\framework\tools\buildUtil\package.py --package=STAKCommands.beta
copy TestCenter.Windows.tar.gz TestCenter.Windows.no_version.tar.gz
python ..\..\framework\tools\buildUtil\setver.py --package=TestCenter.Windows.tar.gz %STC_MAJ_MIN_VERSION% %P4_CHANGELIST%
)</command>
    </hudson.tasks.BatchFile>
    <hudson.tasks.BatchFile>
      <command>if %BUILD_RUN_UNITTEST% == true (
cd bin\Release
python ..\..\framework\tools\testRunner\testRunner.py --runInParallel=8 --ui --uicoverage --archive
rmdir /Q /S unit_test_report
mkdir unit_test_report
copy *.html unit_test_report
mv *_CoverageReport unit_test_report
)</command>
    </hudson.tasks.BatchFile>
