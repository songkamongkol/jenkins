      <command>REM sanity check, make sure no files are checked out from Perforce at the beginning
p4 revert //...

REM set up environment
NET USE %MAPPED_TOOL_DRIVE_LETTER%: %REMOTE_TOOL_ROOT% /PERSISTENT:NO
SET LM_LICENSE_FILE=%LM_LICENSE_FILE%
SET XILINXD_LICENSE_FILE=%XILINXD_LICENSE_FILE%

CALL %XILINX_PATH%\settings64.bat
REM CALL %MAPPED_DRIVE_LETTER%:\win64\Xilinx\Vivado\2014.3\settings64.bat
REM CALL \\storage.cal.ci.spirentcom.com\general\Development\Hardware\FPGA\Tools\Windows\win64\Xilinx\Vivado\2014.3\settings64.bat

@ECHO ON

SET /P LAST_CHANGE= &lt; %WORKSPACE%\changelist.txt


if %P4_CHANGELIST% gtr %LAST_CHANGE% (
    SET LAST_CHANGE=%P4_CHANGELIST%
)


ECHO Start implementation
PUSHD %WORKSPACE%\VivadoBlade
REM PUSHD %WORKSPACE%\VivadoBlade_14_3

ATTRIB -R .\*.xpr

echo %VER_DATECODE% &gt; .\version_%VER_DATECODE%.txt

IF exist myImp.tcl (
    del myImp.tcl
)

ECHO open_project VivadoBlade_14_3.xpr &gt;&gt; myImp.tcl
ECHO set_param general.maxThreads %MAX_CPU_USAGE% &gt;&gt; myImp.tcl
ECHO reset_run impl_2 &gt;&gt; myImp.tcl
ECHO launch_runs impl_2 &gt;&gt; myImp.tcl
ECHO wait_on_run impl_2 &gt;&gt; myImp.tcl
ECHO open_run impl_2 &gt;&gt; myImp.tcl
ECHO set_property BITSTREAM.CONFIG.USR_ACCESS TIMESTAMP [current_design] &gt;&gt; myImp.tcl
REM ECHO set_property BITSTREAM.CONFIG.USR_ACCESS %VER_DATECODE% [current_design] &gt;&gt; myImp.tcl
ECHO write_bitstream ./VivadoBlade_14_3.runs/impl_2/%BITSTREAM_NAME% &gt;&gt; myImp.tcl
ECHO write_verilog -force colossus_iob_imp_funcsim.v -mode funcsim &gt;&gt; myImp.tcl
REM ECHO write_verilog -force -mode timesim â€“sdf_anno true -sdf_file colossus_iob_imp.sdf colossus_iob_imp_timesim.v &gt;&gt; myImp.tcl
ECHO write_verilog -force colossus_iob_imp_timesim.v -mode timesim -sdf_anno true &gt;&gt; myImp.tcl
ECHO write_sdf -force colossus_iob_imp_timesim.sdf &gt;&gt; myImp.tcl
REM ECHO archive_project %WORKSPACE%/%JOB_NAME%-%BUILD_NUMBER%.zip -force -include_config_settings &gt;&gt; myImp.tcl
REM ECHO archive_project %JOB_NAME%-%BUILD_NUMBER%.zip -force -include_config_settings &gt;&gt; myImp.tcl
ECHO close_project &gt;&gt; myImp.tcl
ECHO exit &gt;&gt; myImp.tcl

vivado -mode batch -source ./myImp.tcl

POPD</command>
