export PAD_BUILD_NUMBER=`printf &apos;%04i&apos; $BUILD_NUMBER`

sudo scons reversion_il $RELEASE_NUM.$PAD_BUILD_NUMBER $RELEASE_NUM.$PAD_BUILD_NUMBER</command>
