    <hudson.tasks.Shell>
      <command># Make wireshark ubuntu32 binary package
#echo &quot;Wireshark Ubuntu32 Package&quot;
#pwd
#cd ..
#cd wireshark_ubuntu32
#sudo chmod -R 777 .
#sudo make
#sudo make debian-package

# Tar wireshark.deb, wireshark-common.deb and tshark.deb
echo &quot;tar wireshark debian package&quot;
cd ..
pwd

#fn should be wireshark_1.10.3-spirent-3_i386.deb. fn2 is wireshark_1.10.3-spirent-3_i386, which is the folder name
fn=$(find -name &apos;wireshark_1.10.3-spirent-*_i386.deb&apos;)
fn=wireshark_1.10.3-spirent-*_i386.deb
echo $fn
len=${#fn}
fn2=${fn:0:${#fn}-5}
echo $fn2

mkdir $fn2
cp wireshark_1.10.3-spirent-*_i386.deb $fn2
cp wireshark-common_1.10.3-spirent-*_i386.deb $fn2
cp tshark_1.10.3-spirent-*_i386.deb $fn2

fnend=&quot;.tar.gz&quot;
tar -zcvf $fn2$fnend $fn</command>
    </hudson.tasks.Shell>
