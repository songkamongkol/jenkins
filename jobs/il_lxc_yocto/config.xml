  <builders>
    <hudson.tasks.Shell>
      <command>rm -rf ${JOB_NAME}_changelist_*.txt
echo $JOB_NAME $BUILD_NUMBER $P4_CHANGELIST &gt; $WORKSPACE/${JOB_NAME}_changelist_${P4_CHANGELIST}.txt</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#remove the previous build
#rm -rf $WORKSPACE/build/il/packages/central/testcenter*.*
sudo scons target=packageclean</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># export CCACHE_DISABLE=1 
export SCONSFLAGS=&quot;-j8 ccache=1 distcc=1&quot;

# $WORKSPACE/build/il/scripts/phxbld centralboot_changeling
$WORKSPACE/build/il/scripts/phxbld yocto_lxc

# export PAD_BUILD_NUMBER=`printf &apos;%04i&apos; $BUILD_NUMBER`

# sudo scons reversion_il $RELEASE_NUM.$PAD_BUILD_NUMBER $RELEASE_NUM.$PAD_BUILD_NUMBER</command>
    </hudson.tasks.Shell>
